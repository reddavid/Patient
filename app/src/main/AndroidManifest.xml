<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.patient">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Patient">
        <activity android:name=".NoteActivity"></activity>
        <activity android:name=".PatientSingleItemActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".RegisteredVaccinationActivity" />
        <activity android:name=".ApplyingVaccinationActivity" />
        <activity android:name=".PatientAddActivity" />
        <activity android:name=".PatientListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>